{
    "name": "graphql-typeorm-pg-nodejs",
    "version": "1.0.0",
    "description": "\"setup basic grapghql with typeorm\"",
    "main": "server.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "tsc",
        "typeorm": "ts-node ./node_modules/typeorm/cli.js",
        "migrations:generate": "npx typeorm-ts-node-commonjs migration:generate ./src/migrations/update-post-table -d ./src/configs/database.config.ts",
        "migrations:run": "npm run typeorm migration:run -- -d ./src/configs/database.config.ts",
        "dev": "nodemon server",
        "type": "module",
        "eslint": "eslint ./src",
        "start": "nodemon cluster",
        "format": "prettier --write \"src/**/*.{js,ts}\"",
        "check-format": "prettier --check \"src/**/*.{js,ts}\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NVVLuan/base_graphql_typeorm.git"
    },
    "keywords": [
        "graphql"
    ],
    "author": "LuanNVV",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/NVVLuan/base_graphql_typeorm.git"
    },
    "homepage": "https://github.com/NVVLuan/base_graphql_typeorm.git",
    "dependencies": {
        "apollo-server": "^3.11.1",
        "bcrypt": "^5.1.0",
        "bullmq": "^3.10.0",
        "class-validator": "^0.14.0",
        "cluster": "^0.7.7",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "graphql": "^15.3.0",
        "ioredis": "^5.3.1",
        "jsonwebtoken": "^9.0.0",
        "loadtest": "^5.2.0",
        "nodemailer": "^6.9.1",
        "nodemon": "^2.0.20",
        "os": "^0.1.2",
        "pg": "^8.4.0",
        "reflect-metadata": "^0.1.13",
        "type-graphql": "^1.2.0-rc.1",
        "typeorm": "^0.3.12",
        "uuid": "^9.0.0",
        "voucher-code-generator": "^1.3.0",
        "winston": "^3.8.2"
    },
    "overrides": {
        "typeorm": {
            "glob": "7.2.3"
        }
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/node": "^16.11.10",
        "@types/nodemailer": "^6.4.7",
        "@types/uuid": "^9.0.1",
        "@types/voucher-code-generator": "^1.1.1",
        "@typescript-eslint/eslint-plugin": "^5.54.0",
        "@typescript-eslint/parser": "^5.54.0",
        "dotenv": "^16.0.3",
        "eslint": "^8.35.0",
        "prettier": "2.8.4",
        "ts-node": "10.7.0",
        "typescript": "4.5.2"
    }
}
